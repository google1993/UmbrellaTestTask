{
  "ListenAddr": "0.0.0.0",
  "ListenPort": 5555,
  "TlsEnabled": true,
  "TlsCrt": "/etc/some/path.crt",
  "TlsKey": "/etc/some/path.key",
  "Filters": [
    {
      "field": "type",
      "operator": "equals",
      "value": "event"
    },
    {
      "field": "level",
      "operator": "not_equals",
      "value": "debug"
    }
  ]
}
